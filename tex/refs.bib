@article{belokurov:2007,
  title = {An {{Orphan}} in the ``{{Field}} of {{Streams}}''},
  author = {Belokurov, V. and Evans, N. W. and Irwin, M. J. and {Lynden-Bell}, D. and Yanny, B. and Vidrih, S. and Gilmore, G. and Seabroke, G. and Zucker, D. B. and Wilkinson, M. I. and Hewett, P. C. and Bramich, D. M. and Fellhauer, M. and Newberg, H. J. and Wyse, R. F. G. and Beers, T. C. and Bell, E. F. and Barentine, J. C. and Brinkmann, J. and Cole, N. and Pan, K. and York, D. G.},
  year = {2007},
  month = mar,
  journal = {The Astrophysical Journal},
  volume = {658},
  pages = {337--344},
  issn = {0004-637X},
  doi = {10.1086/511302},
  urldate = {2023-11-15},
  abstract = {We use Sloan Digital Sky Survey Data Release 5 photometry and spectroscopy to study a tidal stream that extends over \textasciitilde 50\textdegree{} in the north Galactic cap. From the analysis of the path of the stream and the colors and magnitudes of its stars, the stream is \textasciitilde 20+7-5 kpc away at its nearest detection (the celestial equator). We detect a distance gradient: the stream is farther away from us at higher declination. The contents of the stream are made up from a predominantly old and metal-poor population that is similar to the globular clusters M13 and M92. The integrated absolute magnitude of the stream stars is estimated to be Mr\textasciitilde -7.5. There is tentative evidence for a velocity signature, with the stream moving at \textasciitilde -40 km s-1 at low declinations and \textasciitilde +100 km s-1 at high declinations. The stream lies on the same great circle as Complex A, a roughly linear association of H I high-velocity clouds stretching over \textasciitilde 30\textdegree{} on the sky, and as Ursa Major II, a recently discovered dwarf spheroidal galaxy. Lying close to the same great circle are a number of anomalous, young, and metal-poor globular clusters, including Palomar 1 and Ruprecht 106.},
  keywords = {Astrophysics,Galaxies: Kinematics and Dynamics,Galaxies: Local Group,Galaxies: Structure},
  annotation = {ADS Bibcode: 2007ApJ...658..337B}
}

